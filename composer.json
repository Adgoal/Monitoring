{
    "name": "adgoal-common/monitoring",
    "type": "library",
    "description": "Adgoal Monitoring common library",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "adgoal-common/base": "^4.0"
    },
    "require-dev": {
        "jakub-onderka/php-console-highlighter": "^0.4",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "mockery/mockery": "^1.2",
        "phan/phan": "4.0.*",
        "php-coveralls/php-coveralls": "^2.4",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-mockery": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master",
        "sensiolabs-de/deptrac-shim": "^0.12.0",
        "symplify/easy-coding-standard": "8.3.*",
        "vimeo/psalm": "^4.7",
        "adgoal/statsd-php-client": "^1.0",
        "domnikl/statsd": "^3.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AdgoalCommon\\Monitoring\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdgoalCommon\\Monitoring\\Tests\\Unit\\": "tests/unit"
        }
    },
    "suggest": {
        "adgoal/statsd-client-bundle": "This bundle helps you to monitor your application. It uses statsd-php-client as Client for the etsy/Statsd API.",
        "adgoal/statsd-php-client": "statsd-php-client is an Open Source, and Object Oriented Client for etsy/statsd written in php.",
        "domnikl/statsd": "A PHP client library for the statistics daemon (statsd) intended to send metrics from PHP applications."
    }
}
